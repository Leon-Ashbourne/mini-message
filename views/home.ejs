<html>
    <head>
        <link rel="stylesheet" href="style.css">
    </head>
    <body>
        <h1>Mini Message Board</h1>
        <div class="container">
            <ul class="messages">
                <% messages.forEach((message) => {%>
                    <%- include("messages", {message: message});%>
                <%});%>
            </ul>
        </div>
    </body>
</html>